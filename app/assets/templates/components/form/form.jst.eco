<div id="panel radius">
  <div id="form-content-region"></div>

  <% if @footer: %>
    <footer>
      <ul class="inline-list">
        <% for button in @buttons: %>
          <li>
            <button data-form-button="<%= button.type %>" class="<%= button.className %>">
              <%= button.text %>
            </button>
          </li>
        <% end %>
      </ul>
    </footer>
  <% end %>
</div>
